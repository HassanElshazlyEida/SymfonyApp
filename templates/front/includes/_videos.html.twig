<div class="album py-5 bg-light">
        <div class="container">

            <div class="row">
               {% for video in videos %}

                <!-- no members -->
                <div class="col-md-4">
                    <div class="card mb-4 shadow-sm">
                        <div align="center" class="embed-responsive embed-responsive-16by9">
                            <iframe class="" src="https://player.vimeo.com/video/{{video.getPath()}}" frameborder="0"
                                allowfullscreen></iframe>
                        </div>
                        <div class="card-body">
                            <h3>{{video.getTitle()}}</h3>
                            <p class="card-text text-danger">Video for <b>MEMBERS</b> only. <a href="pricing.php">Sign
                                    up</a> for one month for free.</p>
                            <div class="d-flex justify-content-between align-items-center">
                                <div class="btn-group">

                                    <i class="fa fa-thumbs-up mr-2"><small class="text-muted">(23)</small></i>
                                    <i class="fa fa-thumbs-down mr-2"><small class="text-muted">(9)</small></i>

                                    <a href="{{path('videoDetails')}}">Comments (32)</a>
                                </div>
                                <small class="text-muted">{{video.getDuration()}}</small>
                            </div>
                        </div>
                    </div>
                </div>
            {% endfor %}

            </div>
        </div>
    </div>